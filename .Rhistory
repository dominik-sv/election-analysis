# Graph for macro hw1
library(ggplot2)
library(tidyr)
p <- seq(0, 10, 0.1)
df <- data.frame(
P = p,
f1 = 140,
f2 = 90.22 * p^(2/5),
f3 = 184.73 * (p / (p + 3))^(2/5)
)
df_long <- pivot_longer(df, cols = c(f1, f2, f3), names_to = "Function", values_to = "Value")
ggplot(df_long, aes(x = Value, y = P, color = Function)) + geom_line(size = 1) +
labs(title = "Agregátní nabídka", x = "P", y = "Y")
ggplot(df_long, aes(x = Value, y = P, color = Function)) + geom_line(size = 1) +
labs(title = "Agregátní nabídka", x = "P", y = "Y") +
theme_dark()
ggplot(df_long, aes(x = Value, y = P, color = Function)) + geom_line(size = 1) +
labs(title = "Agregátní nabídka", x = "P", y = "Y") +
theme_minimal()
ggplot(df_long, aes(x = Value, y = P, color = Function)) +
geom_line(size = 1.2) +
scale_color_manual(
name = "Typ mzdy",
values = c(
"perfektně flexibilní nominální mzdy" = "blue",
"perfektně strnulé nominální mzdy" = "red",
"částečné indexace nominální mzdy" = "darkgreen"
)
) +
labs(
title = "Agregátní nabídka",
x = "Y",
y = "P"
) +
theme_light() +
theme(
plot.title = element_text(size = 18, face = "bold", hjust = 0.5),
axis.title = element_text(size = 14),
legend.title = element_text(size = 13),
legend.position = "bottom"
)
df_long$Function <- recode(df_long$Function,
"f1" = "perfektně flexibilní nominální mzdy",
"f2" = "perfektně strnulé nominální mzdy",
"f3" = "částečné indexace nominální mzdy"
)
